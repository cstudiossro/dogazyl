<body>

<div class="navbar bg-base-100">
    <div class="flex-1">
        <a class="btn btn-ghost normal-case text-xl">
            {% if global.image1 is not empty %}
            <img src="{{ global.image1.one().url() }}" alt="Logo">
            {% else %}
                {{ "Logo" }}
            {% endif %}
        </a>
    </div>
    <div class="flex-none">
        <ul class="menu menu-horizontal px-1">
            {% for item in craft.entries.section("navigation").level(1).all() %}
                {% if item.children is not empty %}
                    <li tabindex="0">
                        <a>
                            {{ item.title }}
                            <svg class="fill-current" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24"><path d="M7.41,8.58L12,13.17L16.59,8.58L18,10L12,16L6,10L7.41,8.58Z"/></svg>
                        </a>
                        <ul class="p-2 bg-base-100">
                            {% for elem in item.children.all() %}
                                <li><a href="{{ elem.entry1.one().url }}">{{ elem.title }}</a></li>
                            {% endfor %}
                        </ul>
                    </li>
                {% else %}
                    <li><a href="{{ item.entry1.one().url }}">{{ item.title }}</a></li>
                {% endif %}
            {% endfor %}
        </ul>
    </div>
</div>

{#{% include "_components/_menu" %}#}

{% block content %} {% endblock %}

